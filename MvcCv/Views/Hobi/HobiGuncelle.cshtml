@model MvcCv.Models.Entity.TblHobilerim

@{
    ViewBag.Title = "HobiGuncelle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Hobi Güncelleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Hobi ID")
    <br />
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />
    @Html.Label("Hobi Açıklaması 1")
    <br />
    @Html.TextBoxFor(x => x.Aciklama1, new { @class = "form-control" })
    <br />
    @Html.Label("Hobi Açıklaması 2")
    <br />
    @Html.TextBoxFor(x => x.Aciklama2, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Güncelle</button>
</form>


